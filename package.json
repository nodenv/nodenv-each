{
  "name": "@nodenv/nodenv-each",
  "version": "1.3.0",
  "description": "A nodenv plugin to run commands with each installed Node version.",
  "homepage": "https://github.com/nodenv/nodenv-each#readme",
  "license": "MIT",
  "contributors": [
    "Chris Eppstein <chris@eppsteins.net> (http://chriseppstein.github.com)",
    "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodenv/nodenv-each.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-each/issues"
  },
  "directories": {
    "bin": "bin",
    "lib": "libexec",
    "test": "test"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "start": "bin/nodenv-each",
    "test": "bats ${CI:+--tap} test",
    "posttest": "npm run lint",
    "lint": "git ls-files bin */*.*sh | xargs shellcheck",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "shellcheck": "^4.1.0"
  }
}
